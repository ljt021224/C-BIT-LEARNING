#include<stdio.h>

int main () {
    int max =0;//初始化最大值
    printf("enter 10 integers\n");
    scanf_s("%d",&max);
    int i=0;
    for (i=1; i<=9; i++) {
         int num =0;
        scanf_s("%d", &num);
        if (num > max)// 交换最大值
        max = num;
    }
    printf("max = %d",max);
    return 0;

}
